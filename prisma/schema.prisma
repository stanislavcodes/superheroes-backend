generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

model Superhero {
  id                 String   @id @default(uuid())
  nickname           String
  real_name          String
  origin_description String   @db.Text
  superpowers        String   @db.Text
  catch_phrase       String
  images             String[]
}
